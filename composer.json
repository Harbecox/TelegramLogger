{
  "name": "harbecox/telegram-logger",
  "description": "Laravel Telegram logging channel",
  "type": "library",
  "keywords": ["laravel", "logging", "telegram", "logger"],
  "license": "MIT",
  "authors": [
    {
      "name": "Harbecox",
      "email": "harbecox@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "monolog/monolog": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Harbecox\\TelegramLogger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Harbecox\\TelegramLogger\\TelegramLoggerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}